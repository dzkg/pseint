Algoritmo horasaminutosysegundos
	Definir horas, minutos, segundos Como Real;
    Escribir "Ingrese la cantidad de horas:";
    Leer horas;
    minutos = horas * 60;
    segundos = horas * 3600;
    Escribir "Equivalente en minutos: ", minutos;
    Escribir "Equivalente en segundos: ", segundos;
FinAlgoritmo
